public Label[] drawMarkers()
{
    Label[] labelArray = new Label[this.markers.Length];
    for (int i = 0; i < this.markers.Length; i++)
    {
        Label label = new Label {
            TextAlign = ContentAlignment.MiddleCenter,
            Font = new Font("Verdana", 17f),
            Width = 70,
            Location = new Point(80 * this.markers[i], (50 * this.tuning.Count) + 20),
            Text = this.markers[i].ToString()
        };
        base.Controls.Add(label);
        labelArray[i] = label;
    }
    return labelArray;
}

 
